{

  "description": "Serves as the skeleton for the DarkWeb Project.",
  "manifest_version": 2,
  "name": "DarkWeb",
  "version": "1.0",
  "homepage_url": "https://github.com/cs3398-betazoid-betamales/CS3398-Betazoid-S2019",
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {

    "default_icon": "icons/sun.svg",
    "browser_style": true,
    "default_popup":"popup/settings.html"
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "linux": "Ctrl+Shift+D"
      },
      "description": "Send a 'toggle-feature' event"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]

}
